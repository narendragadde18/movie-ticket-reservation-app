        <name>validateStartAndEndSeatNumbers</name>
        <order/>
        <published_ref/>
        <script><![CDATA[function onChange(control, oldValue, newValue, isLoading) {
    if (isLoading || newValue == '' || g_form.getValue('start_seat_number') == '' || g_form.getValue('end_seat_number') == '') {
        return;
    }

    var start_seat_number = g_form.getValue('start_seat_number');
    var end_seat_number = g_form.getValue('end_seat_number');
    if (Number(start_seat_number) > Number(end_seat_number)) {
        g_form.setValue('end_seat_number', '');
        g_form.showFieldMsg('end_seat_number', "Start seat number is less than end seat number", true);
    }

}]]></script>
